<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
        
<hibernate-mapping>
    <class name="com.Entity.User" table="tb_User" schema="dbo" catalog="City">
         <id name="id" type="java.lang.Integer">
             <column name="ID"/>
             <generator class="native"></generator>
         </id>
   
    
   <property name="username" type="java.lang.String">
       <column name="USERNAME" />
   </property>
   
   <property name="password" type="java.lang.String">
       <column name="PASSWORD" />
   </property>
 
   <property name="avatar" type="java.lang.String">
       <column name="AVATAR" />
   </property>
   
   <property name="background" type="java.lang.String" ></property>
   
   <property name="registerDate" type="java.util.Date">
       <column name="REGISTER_DATE" />
   </property>
   
   <property name="managerOrNot" type="java.lang.String">
       <column name="MANAGER_ORNOT" />
   </property>
   
   <property name="individualSignature" type="java.lang.String"></property>
   <property name="greatCount" type="java.lang.Integer"></property>
   <property name="answerCount" type="java.lang.Integer"></property>
   <property name="blogCount" type="java.lang.Integer"></property>
   <property name="followerCount" type="java.lang.Integer"></property>
   <property name="followingCount" type="java.lang.Integer"></property>
   <property name="questionCount" type="java.lang.Integer"></property>
   <property name="interestTag" type="java.lang.String"></property>
   <property name="isForbidden" type="java.lang.String"></property>
    </class>
   
   
</hibernate-mapping>