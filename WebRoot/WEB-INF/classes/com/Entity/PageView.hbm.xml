<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping  >

    <class name="com.Entity.PageView" table="tb_PageView" schema="dbo" catalog="City">
       
        <id name="id" type="java.lang.Integer" column="id">
            <generator class="native"/>
        </id>
  
        <property name="time" type="java.util.Date"></property>
        <property name="loginCount" type="java.lang.Integer"></property>
		<property name="pageViewCount" type="java.lang.Integer"></property>
		<property name="articleCount" type="java.lang.Integer"></property>
		<property name="commentCount" type="java.lang.Integer"></property>
		<property name="questionCount" type="java.lang.Integer" ></property>
		<property name="answerCount" type="java.lang.Integer" ></property>
		<property name="registerCount" type="java.lang.Integer" ></property>
		 
		 
    </class>
    

</hibernate-mapping>
